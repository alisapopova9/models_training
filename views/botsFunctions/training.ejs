<form id="form" class="ms-5 mt-3">
    <div class="container">
        <div class="row">
            <div class="col-4">
                <label class="form-label" for="file-upload"></label>
                <input class="form-control" type="file" name="file" id="file-upload" accept=".csv" required onchange="ChangeLoadBtn()"/>
            </div>
            <div class="col">
                <button class="btn btn-primary mt-4 bg-black" id="loadBtn" onclick="EnableBtns()" disabled>Загрузить файл</button>
            </div>
            <small id="fileHelpBlock" class="form-text text-muted">Выберите файл с расширением csv</small>
        </div>
        
        <div class="row">
            <div class="col-2">
                <button class="btn btn-primary mt-4 bg-black" type="submit" id="watchDataBtn" disabled>Посмотреть данные</button>
            </div>
            <div class="col">
                <button class="btn btn-primary mt-4 bg-black" type="submit" id="learnModelBtn" disabled>Обучить модель</button>
            </div>
        </div>
          
        
    </div>

</form>
<div id="DataForLearning"></div>
